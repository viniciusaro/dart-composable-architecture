{
  "editor.formatOnSave": true,
  "dart.lineLength": 100,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.fvm": true,
    "**/.dart_tool": true,
    "**/.idea": true,
    "**/*.iml": true,
  },
  "dart.cliAdditionalArgs": [
    "--enable-experiment=macros"
  ],
  "dart.vmAdditionalArgs": [
    "--enable-experiment=macros"
  ],
  "dart.flutterSdkPath": ".fvm/versions/master"
}